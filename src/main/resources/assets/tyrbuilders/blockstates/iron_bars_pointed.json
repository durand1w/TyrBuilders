{
    "multipart": [
        {   "apply": { "model": "tyrbuilders:iron_bars_pointed_post" }},
        {   "when": { "up": true, "north": true },
            "apply": { "model": "iron_bars_side" }
        },
        {   "when": { "up": true, "east": true },
            "apply": { "model": "iron_bars_side", "y": 90 }
        },
        {   "when": { "up": true, "south": true },
            "apply": { "model": "iron_bars_side_alt" }
        },
        {   "when": { "up": true, "west": true },
            "apply": { "model": "iron_bars_side_alt", "y": 90 }
        },
        {   "when": { "up": true, "north": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside" }
        },
        {   "when": { "up": true, "east": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside_alt" }
        },
        {   "when": { "up": true, "south": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside_alt", "y": 90 }
        },
        {   "when": { "up": true, "west": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside", "y": 270 }
        },
        {   "when": { "up": false, "north": true },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_side" }
        },
        {   "when": { "up": false, "east": true },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_side", "y": 90 }
        },
        {   "when": { "up": false, "south": true },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_side_alt" }
        },
        {   "when": { "up": false, "west": true },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_side_alt", "y": 90 }
        },
        {   "when": { "up": false, "north": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside" }
        },
        {   "when": { "up": false, "east": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside_alt" }
        },
        {   "when": { "up": false, "south": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside_alt", "y": 90 }
        },
        {   "when": { "up": false, "west": false },
            "apply": { "model": "tyrbuilders:iron_bars_pointed_noside", "y": 270 }
        }
    ]
}
