{
    "multipart": [
        {   "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_post" }},
        {   "when": { "north": true },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "tyrbuilders:wrought_iron_bars_jail_noside", "y": 270 }
        }
    ]
}
